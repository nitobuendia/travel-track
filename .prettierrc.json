{
  "$schema": "https://json.schemastore.org/prettierrc",
  "arrowParens": "always",
  "bracketSpacing": false,
  "endOfLine": "lf",
  "jsxSingleQuote": true,
  "semi": true,
  "printWidth": 80,
  "quoteProps": "consistent",
  "singleAttributePerLine": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5"
}
